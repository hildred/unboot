
console --picture splash.pgm ||
set pxe_default_server ${next-server}
cpuid --ext 29 && set arch amd64 || set arch i386
cpuid 6 && set pae yes ||
cpuid --ecx 5 && set vmx yes ||
cpuid --ext --ecx 2 && set svm yes ||
isset ${vmx} || isset ${svm} && set hw_virt yes ||
set space:hex 20
set lf:hex 0a
isset ${video} || set video gtk
set debug 5
set preseed http://${pxe_default_server}/debian/preseed.cfg 


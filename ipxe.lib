
console --picture splash.png || console --picture splash.pgm ||
set pxe_default_server ${next-server}
cpuid --ext 29 && set arch amd64 || set arch i386
cpuid 6 && set pae yes ||
cpuid --ecx 5 && set vmx yes ||
cpuid --ext --ecx 2 && set svm yes ||
isset ${vmx} || isset ${svm} && set hw_virt yes ||
set space:hex 20
set lf:hex 0a
set esc:hex 1b
isset ${video} || set video gtk
isset ${vid} || set vid ${video}
isset ${debug} || set debug 5
isset ${preseed} || set preseed http://${pxe_default_server}/debian/preseed.cfg 
isset ${desktop} || set desktop xfce
isset ${dpreseed} || set dpreseed url=${preseed}
isset ${ddesktop} || set ddesktop desktop=${desktop}
isset ${dvideo} || set dvideo video=vesa:ywrap,mtrr vga=788
isset ${dmode} || set dmode -- quiet

